<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Employed Robots</title>
    <link href="main.css" rel="stylesheet">
  </head>
  <header>
    <h1>Currently Employed Robots</h1>
  </header>
  <body class="user-page">
    {{#employed}}
    <a class="users" id="{{id}}" href="user-{{id}}">
      <img src="{{avatar}}" alt="avatar picture">
      <div class="users-info">
        <h2>{{name}}</h2>
        <p>{{job}}, {{company}}</p>
      </div>
    </a>
    {{/employed}}
  </body>
</html>
